    @RenderBody()
    
    <div id="contact" class="contact">
		<div class="container">
			<div class="footer">
				<div class="blauerHintergrund1">
					<h1 style="color: black;">__</h1>
					<h3 style="color: black; font-weight:900">KONTAKT</h3>
					<p style="color: black;margin-top: 30px">
						Lindenstraße 507 10555 Berlin
					</p>
					<p style="color:black;margin-top:30px">
						Tel.: +49 (0) 456 7890		Fax.: +49 (0) 175 456
					</p>
					<p style="color:black;cursor: pointer;margin-top:30px">info@website.com</p>
					<div class="Bildchen">
						<img src="~/Content/img/kleine Zeichen/facebook.svg" style="color: black; margin-left: 7px; " />
						<img src="~/Content/img/kleine Zeichen/twitter.svg" style="color:black;margin-left:7px;" />
						<img src="~/Content/img/kleine Zeichen/instagram.svg" style="color:black;margin-left:7px;" />
						<img src="~/Content/img/kleine Zeichen/youtube-168.svg" style="color:black;margin-left:7px;" />
					</div>
					<p style="color: black; font-weight: 100;cursor: pointer; margin-top: 53px;">Impressum     Datenschutz     AGB</p>
					<p style="color:black;font-weight:100;">© 2035 Persönliche Lebensberatung. Erstellt mit Wix.com</p>
				</div>
				<div class="blauerHintergrund2">
					<form action="/Home/Form" method="post" id="contact-form">
						<h3 style="color:black;margin-top:81px;font-weight:100;margin-bottom:3px;">Namen eingeben</h3>
						<div class="Textfeld">
							<input id="form-name" name="Name" maxlength="100" placeholder="Name" />
							<div class="errors">
								<div id="name-less-than-3">Your name is to short!</div>
								<div id="name-without-surname">Write a name with surname!</div>
								<div id="name-bigger-than-50">Your name is to long!</div>
							</div>
						</div>
						<h3 style="color: black; margin-top: 10px; font-weight: 100; margin-bottom: 3px;">E-Mail-Adresse eingeben</h3>
						<div class="Textfeld">
							<input id="form-Email" name="Email" maxlength="100" placeholder="E-Mail-Adresse" />
							<div class="errors">
								<div id="Email-with-Space">NO SPACE!</div>
								<div id="Email-without-dog">Write with an </div>
								<div id="Email-without-.com">Write with an .com</div>
								<div id="name-bigger-than-100">Your Email is to long!</div>
							</div>
						</div>
						<h3 style="color: black; margin-top: 10px; font-weight: 100; margin-bottom: 3px;">Betreff eingeben</h3>
						<div class="Textfeld">
							<input id="form-Betreff" name="Topic" placeholder="Betreff" />
							<div class="errors">
								<div id="Betreff-to-short">TO SHORT!</div>
								<div id="Betreff-to-long">TO LONG!</div>
							</div>
						</div>

						<h3 style="color: black; margin-top: 10px; font-weight: 100; margin-bottom: 3px;">Nachricht</h3>
						<div class="Textfeld">
							<textarea id="form-Text" name="Message" placeholder="Nachricht" style="height: 130px; width: 450px; background-color: #4dbdbd; border: solid; "></textarea>
							<div class="errors">
								<div id="Text-to-short">TO SHORT!</div>
								<div id="Text-to-long">TO LONG!</div>
							</div>

						</div>
						<button id="form-submit" class="Main-Button">Absenden</button>
					</form>

				</div>
			</div>
		</div>
	</div>

	<div id="Danke" class="modal" style="display: none;">
		<div id="fade" onclick="closeWindow()" class="fade"></div>
		<div class="dialog">
			<button id="Close-Window-one" onclick="closeWindow()" class="Button-X">X</button>
			<div class="Dialog-Text">Thank you for contacting us, we will get to you soon :)</div>
			<button id="Close-Window-two" onclick="closeWindow()" class="Button-OK">OK</button>

		</div>
	</div>

    
    @* <script src="~/lib/jquery/dist/jquery.min.js"></script> *@
    @* <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script> *@
    <script src="~/js/index.js" asp-append-version="true"></script>
	<script src="~/js/slider.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
